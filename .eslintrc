{
  "env": {
    "es6": true,
    "node": true,
    "jest": true
  },

  "extends": "airbnb-base",
  "parser": "babel-eslint",
  "rules": {
    "import/prefer-default-export": 0,
    "import/no-extraneous-dependencies": 0,
    "new-cap": 0,
    "semi": 0,
    "curly": 0,
    "max-len": 0,
    "no-mixed-operators": 0,
    "arrow-parens": 0,
    "no-confusing-arrow": 0
  }
}
